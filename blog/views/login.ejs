<%- include header %>
	<div class="span10 offset7">
		<form id="form-login" method="post" class="form-login">
			<div class="login-header">Account Login</div>
			<div class="row-fluid">
				<input class="span20 offset2" type="text" name="username" placeholder="Username" />
			</div>
			<div class="row-fluid">
				<input class="span20 offset2" type="password" name="password" placeholder="password" />
			</div>
			<div class="row-fluid">
				<p class="span20 offset2">
					<a href="/reg"><strong>Create New Account</strong></a>
					<a class="forget pull-right" href="javascript:void:(0)">Forgot Password?</a>
				</p>
			</div>
			<div class="row-fluid">
				<button type="submit" class="btn btn-large btn-primary span20 offset2">Sign In</button>
			</div>
		</form>	
	</div>
<%- include footer %>
<script src="javascripts/login.js"></script>
